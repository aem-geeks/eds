{
  "project": "Geeks Edge Deliver Services",
  "plugins": [
    {
      "id": "google-assist",
      "title": "Google Assist",
      "environments": [ "edit","dev", "preview", "live", "prod"],
      "url": "https://www.google.com/search?q=edge+delivery+services",
      "passConfig":true,
      "passReferrer": true,
      "includePaths": [
        "**.docx**"
      ],
      "paletteRect": "top: 50px; bottom: 10px; right: 10px; left: auto; width:400px; height: calc(100vh - 60px)"
    },
    {
      "id": "tools",
      "title": "Tools",
      "isContainer": true
    },
    {
      "containerId": "tools",
      "id": "localize",
      "title": "Localize 2",
      "environments": [ "edit","dev", "preview", "live", "prod" ],
      "url": "/tools/loc/index.html",
      "passReferrer": true,
      "includePaths": [ "**/:x**" ]
    },
    {
      "containerId": "tools",
      "id": "contactus",
      "title": "Contact US",
      "environments": [ "edit","dev", "preview", "live", "prod" ],
      "url": "/tools/contactus/contactus.html?user=aem-geeks",
      "isPalette": true,
      "passReferrer": false,
      "includePaths": [ "**/:x**" ],
      "paletteRect": "top: 50px; bottom: 10px; right: 10px; left: auto; width:500px; height: calc(100vh - 60px)"
    },
    {
      "containerId": "tools",
      "id": "youtube",
      "title": "YouTube",
      "environments": [ "edit","dev", "preview", "live", "prod" ],
      "url": "https://www.youtube.com/@AEMGeeks",
      "passReferrer": true,
      "includePaths": [ "**/:x**" ]
    }
  ]
}