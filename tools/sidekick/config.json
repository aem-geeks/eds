{
  "project": "Geeks franklin",
  "plugins": [
    {
      "id": "google-assist",
      "title": "Google Assist",
      "environments": [ "edit","dev", "preview", "live", "prod"],
      "url": "http://www.google.com",
      "isPalette": false,
      "includePaths": [
        "**.docx**"
      ],
      "paletteRect": "top: 50px; bottom: 10px; right: 10px; left: auto; width:400px; height: calc(100vh - 60px)"
    },
    {
      "id": "tools",
      "title": "Tools",
      "isContainer": true
    },
    {
      "containerId": "tools",
      "id": "localize",
      "title": "Localize",
      "environments": [ "edit","dev", "preview", "live", "prod" ],
      "url": "/tools/loc/index.html",
      "passReferrer": true,
      "includePaths": [ "**/:x**" ]
    },
    {
      "containerId": "tools",
      "id": "youtube",
      "title": "YouTube",
      "environments": [ "edit","dev", "preview", "live", "prod" ],
      "url": "https://www.youtube.com/@AEMGeeks",
      "passReferrer": true,
      "includePaths": [ "**/:x**" ]
    },
    {
      "containerId": "tools",
      "title": "Contact Us",
      "id": "contacttous",
      "environments": ["dev", "preview", "live", "prod"],
      "event": "contact-to-us",
      "url": "/tools/contactus/contactus.html",
      "excludePaths": ["/tools/caas**", "/tools/ost**", "*.json"]
    }
  ]
}